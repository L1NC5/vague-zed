{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "Vague",
  "author": "Aejkatappaja, L1NC5 (original vim theme by vague2k)",
  "accents": [
    "#c48282",
    "#e8b589",
    "#e0a363",
    "#c3c3d5",
    "#aeaed1",
    "#bb9bdb",
    "#6e94b2"
  ],
  "themes": [
    {
      "name": "Vague",
      "appearance": "dark",
      "style": {
        "background": "#141415",
        "syntax": {
          "comment": {
            "color": "#606079",
            "font_style": "italic"
          },
          "comment.doc": {
            "color": "#606079",
            "font_style": "italic"
          },
          "string": {
            "color": "#e8b589",
            "font_style": "italic"
          },
          "string.special": {
            "color": "#e8b589",
            "font_style": "italic"
          },
          "string.escape": {
            "color": "#6e94b2",
            "font_style": "italic"
          },
          "string.regex": {
            "color": "#6e94b2",
            "font_style": "italic"
          },

          "link_text": {
            "color": "#e8b589"
          },
          "link_uri": {
            "color": "#e8b589"
          },
          "text.literal": {
            "color": "#aeaed1"
          },

          "number": {
            "color": "#e0a363"
          },
          "boolean": {
            "color": "#e0a363",
            "font_weight": 700
          },
          "constant": {
            "color": "#aeaed1"
          },

          "character": {
            "color": "#e8b589",
            "font_style": "italic"
          },
          "character.special": {
            "color": "#6e94b2",
            "font_style": "italic"
          },

          "variable.parameter": {
            "color": "#bb9dbd"
          },
          "variable.member": {
            "color": "#9bb4bc"
          },
          "variable.special": {
            "color": "#b4d4cf"
          },
          "property": {
            "color": "#c3c3d5"
          },

          "function": {
            "color": "#c48282"
          },
          "function.call": {
            "color": "#bb9dbd"
          },
          "function.macro": {
            "color": "#aeaed1"
          },
          "function.method": {
            "color": "#c48282"
          },
          "function.method.call": {
            "color": "#9bb4bc"
          },

          "keyword": {
            "color": "#6e94b2"
          },
          "label": {
            "color": "#6e94b2"
          },
          "title": {
            "color": "#6e94b2"
          },
          "operator": {
            "color": "#90a0b5"
          },
          "preproc": {
            "color": "#aeaed1"
          },
          "constructor": {
            "color": "#aeaed1"
          },
          "module": {
            "color": "#aeaed1"
          },
          "builtin": {
            "color": "#b4d4cf"
          },
          "hint": {
            "color": "#7e98e8"
          },

          "type": {
            "color": "#9bb4bc"
          },
          "type.builtin": {
            "color": "#b4d4cf",
            "font_weight": 700
          },
          "enum": {
            "color": "#9bb4bc"
          },
          "namespace": {
            "color": "#9bb4bc"
          },

          "tag": {
            "color": "#9bb4bc"
          },
          "tag.attribute": {
            "color": "#aeaed1"
          },
          "tag.delimiter": {
            "color": "#cdcdcd"
          },

          "attribute": {
            "color": "#aeaed1"
          },
          "attribute.builtin": {
            "color": "#9bb4bc"
          },

          "punctuation": {
            "color": "#cdcdcd"
          },
          "punctuation.special": {
            "color": "#6e94b2"
          },
          "punctuation.delimiter": {
            "color": "#cdcdcd"
          },
          "punctuation.list_marker": {
            "color": "#c48282"
          },

          "emphasis": {
            "color": "#cdcdcd",
            "font_style": "italic"
          },
          "emphasis.strong": {
            "color": "#cdcdcd",
            "font_weight": 700
          }
        },

        "players": [
          {
            "background": "#141415",
            "cursor": "#cdcdcd",
            "selection": "#333738"
          }
        ],

        "link_text.hover": "#7e98e8",

        "error": "#d8647e",
        "error.border": "#d8647e",
        "error.background": "#d8647e1a",

        "warning": "#f3be7c",
        "warning.border": "#f3be7c",
        "warning.background": "#f3be7c1a",

        "hint": "#7e98e8",
        "hint.border": "#7e98e8",
        "hint.background": "#7e98e81a",

        "success": "#7fa563",
        "success.border": "#7fa563",
        "success.background": "#7fa5631a",

        "status_bar.foreground": "#cdcdcd",
        "tab.active_foreground": "#6e94b2",

        "text": "#cdcdcd",
        "text.accent": "#6e94b2",
        "text.disabled": "#606079",
        "text.muted": "#a0a0aa",
        "text.placeholder": "#606079",

        "element.background": "#141415",
        "element.active": "#252530",
        "element.disabled": "#141415",
        "element.hover": "#333738aa",
        "element.selected": "#252530",

        "ghost_element.background": "#141415",
        "ghost_element.disabled": "#141415",
        "ghost_element.hover": "#141415",
        "ghost_element.active": "#252530",
        "ghost_element.selected": "#252530",

        "icon.accent": "#6e94b2",
        "icon.muted": "#606079",
        "icon": "#cdcdcd",
        "icon.disabled": "#606079",
        "icon.placeholder": "#c3c3d5",
        "debugger.accent": "#d8647e",

        "scrollbar_thumb.background": "#33373888",
        "scrollbar.track.background": "#1a1a1a",
        "scrollbar.thumb.hover_background": "#333738aa",
        "scrollbar.track.border": "#191919",
        "scrollbar.thumb.border": "#191919",

        "drop_target.background": "#40506580",

        "editor.active_line.background": "#252530",
        "editor.active_line_number": "#cdcdcd",
        "editor.active_wrap_guide": "#2525301a",
        "editor.background": "#141415",
        "editor.foreground": "#cdcdcd",
        "editor.gutter.background": "#141415",
        "editor.line_number": "#606079",
        "editor.highlighted_line.background": "#252530",
        "editor.invisible": "#252530",
        "editor.subheader.background": "#141415",
        "editor.wrap_guide": "#2525300d",
        "editor.document_highlight.bracket_background": "#333738",
        "editor.document_highlight.read_background": "#333738",
        "editor.document_highlight.write_background": "#333738",

        "elevated_surface.background": "#141415",
        "panel.background": "#141415",

        "search.match_background": "#4050651a",

        "status_bar.background": "#141415",
        "surface.background": "#141415",
        "tab.active_background": "#141415",
        "tab.inactive_background": "#141415",
        "tab_bar.background": "#141415",
        "title_bar.background": "#141415",
        "toolbar.background": "#141415",

        "border": "#252530",
        "border.variant": "#878787",
        "border.selected": "#6e94b2",
        "border.disabled": "#252530",
        "border.focused": "#405065",
        "border.transparent": "#00000000",

        "terminal.ansi.black": "#252530",
        "terminal.ansi.bright_black": "#606079",
        "terminal.ansi.red": "#d8647e",
        "terminal.ansi.bright_red": "#e08398",
        "terminal.ansi.green": "#7fa563",
        "terminal.ansi.bright_green": "#99b782",
        "terminal.ansi.yellow": "#f3be7c",
        "terminal.ansi.bright_yellow": "#f5cb96",
        "terminal.ansi.blue": "#6e94b2",
        "terminal.ansi.bright_blue": "#8ba9c1",
        "terminal.ansi.magenta": "#bb9dbd",
        "terminal.ansi.bright_magenta": "#c9b1ca",
        "terminal.ansi.cyan": "#aeaed1",
        "terminal.ansi.bright_cyan": "#bebeda",
        "terminal.ansi.white": "#cdcdcd",
        "terminal.ansi.bright_white": "#d7d7d7",
        "terminal.background": "#141415",
        "terminal.foreground": "#cdcdcd"
      }
    }
  ]
}
